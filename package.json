{
  "name": "misevi",
  "version": "0.5.2",
  "description": "Misevi is a component library designed for creating tabletop rpg character sheets.",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "repository": "git://github.com/zogar1993/misevi.git",
  "keywords": [
    "role playing game",
    "rpg",
    "components",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/zogar1993/misevi/issues"
  },
  "homepage": "https://github.com/zogar1993/misevi#readme",
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.modern.js",
    "dist/index.modern.js.map",
    "dist/index.d.ts",
    "dist/components/*"
  ],
  "scripts": {
    "start": "yarn storybook",
    "build": "npm run build:ts",
    "build:ts": "npx rollup -c",
    "build:clean": "rm -rf ./dist/*",
    "test": "jest",
    "storybook": "start-storybook -p 6006 --no-dll --no-manager-cache",
    "build-storybook": "build-storybook --no-dll --no-manager-cache",
    "format": "prettier --write 'src/**/*.ts' './src/**/*.tsx'"
  },
  "peerDependencies": {
    "react": ">= 17",
    "react-dom": ">= 17",
    "styled-components": ">= 5"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@storybook/addon-a11y": "6.5.10",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-docs": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/react": "6.5.10",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "28.1.6",
    "@types/node": "18.6.5",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.25",
    "jest": "28.1.3",
    "jest-transform-stub": "2.0.0",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "2.77.2",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.32.1",
    "rollup-plugin-url": "3.0.1",
    "styled-components": "5.3.5",
    "ts-jest": "28.0.7",
    "ts-mockito": "2.6.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "jest-environment-jsdom": "28.1.3",
    "react-image-file-resizer": "0.4.8",
    "react-is": "18.2.0",
    "style-inject": "0.3.0"
  }
