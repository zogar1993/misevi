{
  "name": "misevi",
  "version": "0.4.14",
  "description": "Misevi is a component library designed for creating tabletop rpg character sheets.",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "repository": "git://github.com/zogar1993/misevi.git",
  "keywords": [
    "role playing game",
    "rpg",
    "components",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/zogar1993/misevi/issues"
  },
  "homepage": "https://github.com/zogar1993/misevi#readme",
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.modern.js",
    "dist/index.modern.js.map",
    "dist/index.d.ts",
    "dist/components/*"
  ],
  "scripts": {
    "start": "yarn storybook",
    "build": "npm run build:ts",
    "build:ts": "npx rollup -c",
    "build:clean": "rm -rf ./dist/*",
    "test": "jest",
    "storybook": "start-storybook -p 6006 --no-dll --no-manager-cache",
    "build-storybook": "build-storybook --no-dll --no-manager-cache",
    "format": "prettier --write 'src/**/*.ts' './src/**/*.tsx'"
  },
  "peerDependencies": {
    "react": ">= 17",
    "react-dom": ">= 17",
    "styled-components": ">= 5"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/preset-env": "7.16.11",
    "@storybook/addon-a11y": "6.4.19",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-docs": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/react": "6.4.19",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "14.18.12",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.23",
    "jest": "27.5.1",
    "jest-svg-transformer": "1.0.0",
    "postcss": "8.4.7",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "2.68.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.31.2",
    "rollup-plugin-url": "3.0.1",
    "styled-components": "5.3.3",
    "ts-jest": "27.1.3",
    "ts-mockito": "2.6.1",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "react-image-file-resizer": "0.4.2",
    "react-is": "17.0.1",
    "style-inject": "0.3.0",
    "yarn": "^1.22.17"
  }
