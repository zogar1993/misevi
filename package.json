{
  "name": "misevi",
  "version": "0.1.0",
  "description": "Misevi is a component library designed for creating tabletop rpg character sheets.",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "repository": "git://github.com/zogar1993/misevi.git",
  "keywords": [
    "role playing game",
    "rpg",
    "components",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/zogar1993/misevi/issues"
  },
  "homepage": "https://github.com/zogar1993/misevi#readme",
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.modern.js",
    "dist/index.modern.js.map",
    "dist/index.d.ts",
    "dist/components/*",
    "dist/styles/*"
  ],
  "scripts": {
    "start": "npm run storybook",
    "build": "npm run build:ts",
    "build:ts": "npx rollup -c",
    "build:clean": "rm -rf ./dist/*",
    "test": "react-scripts test",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll",
    "format": "prettier --write 'src/**/*.ts' './src/**/*.tsx'"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.7",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-docs": "^6.0.28",
    "@storybook/addon-essentials": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/react": "6.0.28",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.1",
    "@testing-library/user-event": "12.2.0",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.7",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/styled-components": "5.1.4",
    "prettier": "2.2.0",
    "rollup": "2.33.3",
    "rollup-plugin-postcss": "3.1.8",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.29.0",
    "rollup-plugin-url": "3.0.1",
    "ts-mockito": "2.6.1",
    "typescript": "4.1.2"
  }
}
