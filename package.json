{
  "name": "misevi",
  "version": "0.1.10",
  "description": "Misevi is a component library designed for creating tabletop rpg character sheets.",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "repository": "git://github.com/zogar1993/misevi.git",
  "keywords": [
    "role playing game",
    "rpg",
    "components",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/zogar1993/misevi/issues"
  },
  "homepage": "https://github.com/zogar1993/misevi#readme",
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.modern.js",
    "dist/index.modern.js.map",
    "dist/index.d.ts",
    "dist/components/*",
    "dist/styles/*"
  ],
  "scripts": {
    "start": "npm run storybook",
    "build": "npm run build:ts",
    "build:ts": "npx rollup -c",
    "build:clean": "rm -rf ./dist/*",
    "test": "jest",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll",
    "format": "prettier --write 'src/**/*.ts' './src/**/*.tsx'"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "peerDependencies": {
    "styled-components": ">= 5"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/preset-env": "7.12.7",
    "@storybook/addon-actions": "6.1.9",
    "@storybook/addon-docs": "6.1.9",
    "@storybook/addon-essentials": "6.1.9",
    "@storybook/addon-links": "6.1.9",
    "@storybook/react": "6.1.9",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.2.2",
    "@types/jest": "^26.0.16",
    "@types/node": "14.14.10",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/styled-components": "5.1.4",
    "babel-jest": "26.6.3",
    "babel-loader": "8.1.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "rollup": "2.34.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "3.1.8",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.29.0",
    "rollup-plugin-url": "3.0.1",
    "styled-components": "5.2.1",
    "ts-jest": "^26.4.4",
    "ts-mockito": "2.6.1",
    "typescript": "4.1.2"
  }
}
